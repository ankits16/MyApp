      "program": "${workspaceFolder}/media_processing_services/manage.py",
      "args": ["runserver", "8003"],
      "django": true,
      "console": "integratedTerminal"
    },
    {
      "name": "Celery : MediaProcessingService",
      "type": "python",
      "request": "launch",
      "program": "/Users/ankit/opt/anaconda3/envs/myapp/bin/celery",
      "args": [
        "-A",
        "media_processing_services",
        "worker",
        "--loglevel=info",
        "--queues=transcription"
